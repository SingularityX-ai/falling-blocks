      <memberdef kind="function" id="classGameRunner_1a60b42ae79f0fc030d0ef8a31d10ae960" prot="public" static="no" const="no" explicit="no" inline="yes" virt="non-virtual">
        <type>void</type>
        <definition>void GameRunner.Update</definition>
        <argsstring>()</argsstring>
        <name>Update</name>
